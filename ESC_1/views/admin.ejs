<% include ../partials/header  %>
<link href="https://fonts.googleapis.com/css?family=Russo+One&display=swap" rel="stylesheet">
<link href="/stylesheets/admin.css" rel="stylesheet" type="text/css">
<div class="main">
        <div style="margin:0 auto;" class="container" id="contain1">
       <% if(data){ %>
        <div id="head-div">
<h2 align="center">NEW APPLICATIONS</h2>
        </div>                
<div class="row" style="display:flex;flex-wrap:wrap; text-align:center; margin-top:5em;">    
                                <% data.forEach(function(data){ %>
                            <div class="col-md-3 col-sm-3">    
                                <div class="thumbnail" style="width:140px;">
                                <img style="width:90px;" src="data:image/png;base64,">
                                <div class="caption">
                                <h4> <%= data.Company %> </h4>
                                
                                
                                <p style="color:blue;"><%= (data.time).toDateString() %></p>
                            </div>
                                
                                <p><a href="/info/<%= data._id %>" class="btn btn-primary" style="width:5em;height:1.7em;padding:0;">More Info</a></p>
                                <p><form action="/delete/<%= data._id %>?_method=DELETE" method="POST"><input type="submit" class="btn btn-danger" style="width:5em;height:1.7em;padding:0;" value="Delete"></form></p>
                                <p><a href="/approve/<%= data._id %>" class="btn btn-success" style="width:5em;height:1.7em;padding:0;">Approve</a></p>    
                                <p><a href="/edit/<%= data._id %>" class="btn btn-warning" style="width:5em;height:1.7em;padding:0;">Edit</a></p>
                                
                        </div>
                        </div>
                        <% }) %>
                    </div>
       <% } else{ %>

<h2>NO NEW APPLICATION</h2>
       <% } %>

</div>
</div>
<% include ../partials/footer  %>